{
  "mcpServers": {
    "imdb-ratings-tool": {
      "command": "docker",
      "args": ["exec", "-i", "imdb-ratings-tool-mcp-server-1", "python", "mcp_server.py"],
      "env": {}
    }
  },
  "mcpServers_local": {
    "imdb-ratings-tool": {
      "command": "python",
      "args": ["mcp_server.py"],
      "env": {
        "DATABASE_URL": "postgresql://postgres:password@localhost:5433/imdb_ratings"
      }
    }
  }
}